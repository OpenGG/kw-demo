    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.3",
    "ansi-colors-es6": "^5.0.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.5.0",
    "fast-glob": "^3.2.7",
    "less": "^4.1.2",
    "sass": "^1.45.1",
    "typescript": "^4.5.4",

{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@ks/kw/*": [
        "./k/*"
      ],
      "@ks/kwcolor/*": [
        "./k/*"
      ]
    },
    "outDir": "./dist-ts"
  },
  "include": [
    "./typings",
    "./src"
  ]
}